{
  "name": "selector-2-regexp",
  "version": "1.0.0",
  "description": "Convert CSS Selector to a Regexp string for searching matched elements in HTML.",
  "engines": {
    "node": "^12 || ^14 || >=16.0.0"
  },
  "bin": {
    "s2r": "bin/selector2regexp"
  },
  "main": "./dist/index.js",
  "scripts": {
    "build": "npm run clean && tsc -p .",
    "clean": "rm -rf ./dist",
    "lint": "eslint ./**/*.ts",
    "lint:fix": "eslint --fix ./**/*.ts",
    "pretest": "npm run build",
    "test": "jest",
    "preversion": "npm run test",
    "version": "npm run build",
    "postversion": "git push && git push --tags",
    "commit": "git-cz"
  },
  "keywords": [
    "s2r",
    "regexp",
    "CSS",
    "selector",
    "HTML"
  ],
  "author": "Yoshiro Matsumoto <yoshiro.public@gmail.com>",
  "license": "MIT",
  "bug": {
    "url": "https://github.com/m-yoshiro/Selector2Regexp/issues"
  },
  "homepage": "https://github.com/m-yoshiro/Selector2Regexp",
  "dependencies": {
    "css-tree": "^1.1.2",
    "deep-equal": "^2.0.5",
    "yargs": "^16.2.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.4.2",
    "@commitlint/config-conventional": "^17.4.2",
    "@commitlint/cz-commitlint": "^17.4.2",
    "@types/css-tree": "^1.0.5",
    "@types/deep-equal": "^1.0.1",
    "@types/jest": "^26.0.20",
    "@types/pretty-format": "^24.3.0",
    "@typescript-eslint/eslint-plugin": "^4.15.1",
    "@typescript-eslint/parser": "^4.15.1",
    "commitizen": "^4.3.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^7.20.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.1.5",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^4.3.1",
    "eslint-plugin-standard": "^5.0.0",
    "husky": "^8.0.3",
    "jest": "^26.6.3",
    "prettier": "^2.2.1",
    "ts-jest": "^26.5.1",
    "typescript": "^4.1.5"
  },
  "config": {
    "commitizen": {
      "path": "@commitlint/cz-commitlint"
    }
  }
}
